{
	"schemaVersion": 1,
	"id": "theendupdate",
	"version": "${version}",
	"name": "The End Update",
	"summary": "by: Ritro",
	"description": "An expansion mod for The End dimension.",
	"authors": [
		"Ritro"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/ritro-the-end-update",
		"sources": "https://github.com/Ritro/TheEndUpdate"
	},
	"license": "MIT",
	"icon": "assets/theendupdate/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.theendupdate.TemplateMod"
		],
		"client": [
			"com.theendupdate.TemplateModClient"
		]
	},
	"accessWidener": "theendupdate.accesswidener",
	"mixins": [
		"template-mod.mixins.json",
		{
			"config": "template-mod.client.mixins.json",
			"environment": "client"
		}
	],
	"custom": {
		"fabric-resource-loader-v0:resource_packs": [
			{
				"id": "theendupdate:default",
				"enabled_by_default": true
			}
		]
	},
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}